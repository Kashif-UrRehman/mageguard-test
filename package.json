{
  "name": "mageguard",
  "version": "1.0.0",
  "description": "Cypress automation tests for Mage Guard application",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "cy:open": "cypress open",
    "cy:run:headed": "cypress run --headed --browser chrome",
    "cy:run:report": "cypress run --reporter mochawesome",
    "merge:reports": "mochawesome-merge cypress/reports/mochawesome/*.json > cypress/reports/mochawesome/report.json",
    "generate:report": "marge cypress/reports/mochawesome/report.json --reportDir cypress/reports/mochawesome",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^15.3.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.0",
    "prettier": "^3.6.2"
  }
}